{
    "extends": [
      "react-app",
      "airbnb",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "jsx-a11y",
      "prettier"
    ],
    "rules": {
      "import/prefer-default-export": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "prettier/prettier": [
        "warn", {
          "singleQuote": true,
          "semi": false,
          "arrowParens": "avoid",
          "trailingComma": "es5",
          "jsxBracketSameLine": true,
          "printWidth": 120 
        }
      ]
    }
  }